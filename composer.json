{
  "name": "workofstan/mycms",
  "description": "MyCMS - Brief MVC framework for interactive websites including general administration.",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/WorkOfStan/tools"
    }
  ],
  "require": {
    "php": ">=7.2 <8.6",
    "ext-session": "*",
    "ext-mbstring": "*",
    "godsdev/tools": "dev-chore/update-phpunit",
    "latte/latte": ">=2.4.6 <2.11.3",
    "nette/utils": "^2.4.8 || ^3.2.2 || ^4.0.5",
    "symfony/yaml": "^4 || ^5",
    "tracy/tracy": "^2.4.10",
    "webmozart/assert": "^1.10.0",
    "workofstan/backyard": "^3.4.2 || ^4.1.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.52 || ^9 || ^10 || ^11 || ^12",
    "robmorgan/phinx": "^0.12.13 || ^0.13.4 || ^0.14.0 || ^0.15.5 || ^0.16.5",
    "swiftmailer/swiftmailer": "^6.3"
  },
  "suggest": {
    "ext-dom": "for Tools",
    "ext-iconv": "for Tools::webalize()",
    "ext-zip": "for Admin's media section",
    "ext-zlib": "for Admin's media section"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Stanislav Rejthar",
      "email": "rejthar@stanislavrejthar.com"
    },
    {
      "name": "Karel Seidl",
      "email": "seidl@gods.cz"
    }
  ],
  "support": {
    "email": "rejthar@stanislavrejthar.com",
    "issues": "https://github.com/WorkOfStan/mycms/issues",
    "source": "https://github.com/WorkOfStan/mycms"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "WorkOfStan\\MyCMS\\": [
        "classes/"
      ]
    },
    "files": [
      "classes/ThrowableFunctions/ThrowablePHPFunctions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WorkOfStan\\MyCMS\\Test\\": [
        "Test/"
      ],
      "WorkOfStan\\mycmsprojectnamespace\\": [
        "dist/classes/"
      ]
    }
  }
}
