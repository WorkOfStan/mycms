{* template latte where app version should be used instead of library version *}
{layout '@inheritance-prefer-app-layout.latte'}
{block htmlbody}
    App template overriding library template -
    <h1>{$htmlbody}</h1>
    <h2>Prefer app:</h2>{include 'inherite.latte', latte => 'inheritance-prefer-app-include.latte'}
    <h2>App only:</h2>{include 'inherite.latte', latte => 'inc-inheritance-app-only.latte'}
    <h2>Library only:</h2>{include 'inherite.latte', latte => 'inheritance-library-only-include.latte'}
{/block}
